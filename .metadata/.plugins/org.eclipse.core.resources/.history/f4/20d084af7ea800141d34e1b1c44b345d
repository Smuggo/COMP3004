package view;

import javax.swing.JInternalFrame;

public class ViewManager {
	
	MagicRealmWindow lWindow;
	
	MenuView lMenu;
	
	
	public ViewManager(){
		lWindow = new MagicRealmWindow();
		
		lMenu = new MenuView();
		lWindow.add(lMenu);
		lMenu.setVisible(true);
		lMenu.setSelected(true);
	}
	
	

	
	
}
