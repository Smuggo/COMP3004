package model;

import client.ClientApp;
import server.ServerApp;

public class GameHub {

	private boolean lHasServer;
	private ServerApp lServerApp;
	private ClientApp lClientApp;
	
	public GameHub(boolean aHasServer){
		lHasServer = aHasServer;
		
		if(lHasServer){
			lServerApp = new ServerApp();
		}
		lClientApp = new ClientApp();
		
	}
	
	public void start(){
		while(true){
			
			
		}
	}
}
